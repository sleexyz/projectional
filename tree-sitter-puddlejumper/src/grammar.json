{
  "name": "puddlejumper",
  "rules": {
    "document": {
      "type": "SYMBOL",
      "name": "_block"
    },
    "_block": {
      "type": "REPEAT1",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "node"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "newline"
              },
              {
                "type": "BLANK"
              }
            ]
          }
        ]
      }
    },
    "node": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "content"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "children"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "children": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_indent"
        },
        {
          "type": "SYMBOL",
          "name": "_block"
        },
        {
          "type": "SYMBOL",
          "name": "_dedent"
        }
      ]
    },
    "content": {
      "type": "PATTERN",
      "value": "[^\\n]+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "_indent"
    },
    {
      "type": "SYMBOL",
      "name": "_dedent"
    },
    {
      "type": "SYMBOL",
      "name": "newline"
    }
  ],
  "inline": [],
  "supertypes": []
}

